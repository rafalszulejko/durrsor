{
  "name": "durrsor",
  "displayName": "durrsor",
  "description": "ai-powered code editor",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "durrsor.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "durrsor-sidebar",
          "title": "Durrsor",
          "icon": "media/durr.svg"
        }
      ]
    },
    "views": {
      "durrsor-sidebar": [
        {
          "type": "webview",
          "id": "durrsor.sidePanel",
          "name": "Durrsor Assistant"
        }
      ]
    },
    "configuration": {
      "title": "Durrsor",
      "properties": {
        "durrsor.apiKeyOpenAI": {
          "type": "string",
          "default": "",
          "description": "API key for OpenAI services"
        },
        "durrsor.awsRegion": {
          "type": "string",
          "default": "",
          "description": "AWS Region for Amazon Bedrock services"
        },
        "durrsor.amazonBedrockAccessKey": {
          "type": "string",
          "default": "",
          "description": "Access Key for Amazon Bedrock services"
        },
        "durrsor.amazonBedrockSecretAccessKey": {
          "type": "string",
          "default": "",
          "description": "Secret Access Key for Amazon Bedrock services"
        },
        "durrsor.bigModel": {
          "type": "string",
          "default": "gpt-4o",
          "enum": [
            "gpt-4o",
            "us.anthropic.claude-3-5-sonnet-20241022-v2:0",
            "anthropic.claude-3-7-sonnet-20250219-v1:0"
          ],
          "enumDescriptions": [
            "GPT-4o - OpenAI's most capable model",
            "Claude 3.5 Sonnet - Anthropic's powerful model via Amazon Bedrock",
            "Claude 3.7 Sonnet - Anthropic's most capable model via Amazon Bedrock"
          ],
          "description": "Select the high-capability model to use for code generation and analysis"
        },
        "durrsor.smallModel": {
          "type": "string",
          "default": "gpt-4o-mini",
          "enum": [
            "gpt-4o-mini",
            "anthropic.claude-3-5-haiku-20241022-v1:0"
          ],
          "enumDescriptions": [
            "GPT-4o-mini - Smaller, faster OpenAI model",
            "Claude 3.5 Haiku - Smaller, faster Anthropic model via Amazon Bedrock"
          ],
          "description": "Select the smaller, faster model to use for agent logic and summaries"
        }
      }
    }
  },
  "extensionDependencies": [
    "vscode.git"
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "compile:fast": "node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "package:fast": "node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/diff": "^7.0.1",
    "@types/markdown-it": "^14.1.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.21.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/credential-provider-node": "^3.758.0",
    "@aws-sdk/types": "^3.734.0",
    "@langchain/community": "^0.3.35",
    "@langchain/core": "^0.3.42",
    "@langchain/langgraph": "^0.2.54",
    "@langchain/openai": "^0.4.4",
    "@smithy/eventstream-codec": "^2.2.0",
    "@smithy/protocol-http": "^3.3.0",
    "@smithy/signature-v4": "^2.3.0",
    "@smithy/util-utf8": "^2.3.0",
    "diff": "^7.0.0",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "markdown-it-highlightjs": "^4.2.0",
    "openai": "^4.86.1"
  }
}
