{
  "name": "durrsor",
  "displayName": "durrsor",
  "description": "ai-powered code editor",
  "version": "0.0.1",
  "type": "commonjs",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "durrsor.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "durrsor-sidebar",
          "title": "Durrsor",
          "icon": "media/durr.svg"
        }
      ]
    },
    "views": {
      "durrsor-sidebar": [
        {
          "type": "webview",
          "id": "durrsor.sidePanel",
          "name": "Durrsor Assistant"
        }
      ]
    },
    "configuration": {
      "title": "Durrsor",
      "properties": {
        "durrsor.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for the Durrsor service"
        },
        "durrsor.bigModel": {
          "type": "string",
          "default": "gpt-4o",
          "enum": ["gpt-4o"],
          "enumDescriptions": [
            "GPT-4o - OpenAI's most capable model"
          ],
          "description": "Select the high-capability model to use for code generation and analysis"
        },
        "durrsor.smallModel": {
          "type": "string",
          "default": "gpt-4o-mini",
          "enum": ["gpt-4o-mini"],
          "enumDescriptions": [
            "GPT-4o-mini - Smaller, faster OpenAI model"
          ],
          "description": "Select the smaller, faster model to use for agent logic and summaries"
        }
      }
    }
  },
  "extensionDependencies": [
    "vscode.git"
  ],
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "compile:fast": "node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "package:fast": "node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/diff": "^7.0.1",
    "@types/markdown-it": "^14.1.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.21.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@langchain/community": "^0.3.34",
    "@langchain/core": "^0.3.42",
    "@langchain/langgraph": "^0.2.54",
    "@langchain/openai": "^0.4.4",
    "diff": "^7.0.0",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "markdown-it-highlightjs": "^4.2.0",
    "openai": "^4.86.1"
  }
}
